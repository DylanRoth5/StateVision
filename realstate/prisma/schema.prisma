generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model RealEstate {
  id Int @id @default(autoincrement())
  title String
  description String?
  price Int?
  type String?
  purpose String?
  createdAt DateTime @default(now()) @db.Timestamptz(6)
  updatedAt DateTime @updatedAt

  @@map("real_states")
}